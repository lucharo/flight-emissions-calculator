    "postcss": "^8.4.24",
    "svelte-check": "^3.4.6",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@types/leaflet": "^1.9.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "svelte": "^4.0.5",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.3.0",
    "tailwindcss-animate": "^1.0.7"
