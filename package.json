    "update-airports:verbose": "python scripts/update_airports.py --verbose",
    "build:prod": "vite build --mode production"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@tsconfig/svelte": "^5.0.0",
    "@types/node": "^20.11.24",
